{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "types": ["@astrojs/image/client", "jest"],
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "@components/*": ["src/components/*"],
            "@layouts/*": ["src/layouts/*"],
            "@api/*": ["src/api/*"],
            "@pages/*": ["src/pages/*"]
        }
    }
}
